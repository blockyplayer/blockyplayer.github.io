def add(num1, num2):
    return float(num1 + num2)

def subtract(num1, num2):
    return float(num1 - num2)

def multiply(num1, num2):
    return float(num1 * num2)

def divide(num1, num2):
    return float(num1 / num2)

def sqrt(num1, num2):
    return float(num1 ** (1/num2))

def index(num1, num2):
    return float(num1 ** num2)